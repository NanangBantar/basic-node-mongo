<%- include('../partials/head'); %>
    <div class="auth pt-4">
        <div class="auth-container px-4">
            <div class="card">
                <header class="auth-header">
                    <h1 class="auth-title">
                        <div class="logo">
                            <span class="l l1"></span>
                            <span class="l l2"></span>
                            <span class="l l3"></span>
                            <span class="l l4"></span>
                            <span class="l l5"></span>
                        </div> ModularAdmin
                    </h1>
                </header>
                <div class="auth-content">
                    <p class="text-center">REGISTER NEW ACCOUNT</p>
                    <form id="login-form">
                        <div class="form-group">
                            <label>Email</label>
                            <input type="email" class="form-control underlined" name="email" placeholder="Your email"
                                required>
                        </div>
                        <div class="form-group">
                            <label>Password</label>
                            <input type="password" class="form-control underlined" name="password"
                                placeholder="Your password" required>
                        </div>
                        <div class="form-group">
                            <button type="submit" class="btn btn-block btn-primary">Create</button>
                        </div>
                        <div class="form-group">
                            <a href="/" class="btn btn-block btn-primary">Already have an account</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div class="ref" id="ref">
        <div class="color-primary"></div>
        <div class="chart">
            <div class="color-primary"></div>
            <div class="color-secondary"></div>
        </div>
    </div>
    <script src="js/vendor.js"></script>
    <script src="js/app.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.24.0/axios.min.js"
        integrity="sha512-u9akINsQsAkG9xjc1cnGF4zw5TFDwkxuc9vUp5dltDWYCSmyd0meygbvgXrlc/z7/o4a19Fb5V0OUE58J7dcyw=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="js/pages/create_account/submit.js"></script>
    <div class="modal fade" id="confirm-modal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"><i class="fa fa-warning"></i> Alert</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">Ã—</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="result-message">
                    </p>
                </div>
            </div>
        </div>
    </div>

    </body>

    </html>